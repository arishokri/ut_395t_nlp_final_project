{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Train",
      "type": "debugpy",
      "request": "launch",
      "program": "run.py",
      "console": "internalConsole",
      "args": [
        "--do_train",
        "--ablations",
        "${input:ablations}",
        "--max_train_samples",
        "1000"
      ]
    },
    {
      "name": "Eval",
      "type": "debugpy",
      "request": "launch",
      "program": "run.py",
      "console": "internalConsole",
      "args": ["--do_eval", "--max_eval_samples", "1000"]
    }
  ],
  "inputs": [
    {
      "id": "ablations",
      "type": "pickString",
      "options": ["none", "q_only", "p_only"],
      "description": "Pick an ablations strategy."
    }
  ]
}
